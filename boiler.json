{
  "name": "tvnamer",
  "version": "0.1.2",
  "description": "A utility to rename and move tv shows",
  "authors": "Nic Aitch <nic@nicinabox.com>",
  "dependencies": {
    "python": ""
  },
  "license": "MIT",
  "post_install": [
    "wget --no-check-certificate https://bitbucket.org/pypa/setuptools/raw/bootstrap/ez_setup.py -O - | python",
    "easy_install tvnamer"
  ],
  "symlink": {
    "/boot/plugins/custom/tvnamer/config/tvnamer.json": "~/.tvnamer.json"
  }
}
